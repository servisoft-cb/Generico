  object qCheque: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT Dbcontas.NomeConta, Dbcheque.NumCheque, Dbcheque.DtEmissa' +
        'o, Dbcheque.VlrTotal, Dbcheque.DtEntrada, Dbcheque.DtPrevista, D' +
        'bcheque.Nominal, Dbcontas.CodConta, Dbcheque.Filial, Dbcheque.Nu' +
        'mMov'
      'FROM "dbCheque.db" Dbcheque'
      '   INNER JOIN "dbContas.DB" Dbcontas'
      '   ON  (Dbcheque.CodConta = Dbcontas.CodConta)  '
      ' ')
