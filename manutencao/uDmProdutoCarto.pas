  cdsProdutoCartoFDVISOR.AsString         := '0';
end;

procedure TdmProdutoCarto.cdsProdutoCartoCalcFields(DataSet: TDataSet);
begin
  if not cdsProdutoCartoTIPO.IsNull then
    case cdsProdutoCartoTIPO.AsInteger of
      1: cdsProdutoCartoclTIPO.AsString := 'CAIXA NORMAL';
      2: cdsProdutoCartoclTIPO.AsString := 'TAMPA ACOPLADA';
      3: cdsProdutoCartoclTIPO.AsString := 'CARTUCHO';
      4: cdsProdutoCartoclTIPO.AsString := 'SACOLA';
      5: cdsProdutoCartoclTIPO.AsString := 'FACA';
    end;
end;

procedure TdmProdutoCarto.dspProdutoUpdateError(Sender: TObject;
  DataSet: TCustomClientDataSet; E: EUpdateError; UpdateKind: TUpdateKind;
  var Response: TResolverResponse);
begin
  Response := rrAbort;
  raise exception.Create(E.Message);
end;

