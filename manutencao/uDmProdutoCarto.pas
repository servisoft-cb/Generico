procedure TdmProdutoCarto.cdsProdutoCartoFDNewRecord(DataSet: TDataSet);
begin
  cdsProdutoCartoFDPINCA.AsString         := 'S';
  cdsProdutoCartoFDPINCA_TAM.AsFloat      := 1.2;
  cdsProdutoCartoFDTrava.AsString         := 'N';
